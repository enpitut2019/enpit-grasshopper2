<h1>Ranking#show</h1>
<p>Find me in app/views/ranking/show.html.erb</p>

<style>
td {
  border: solid 1px;
}
</style>

<%= @myId %>
<%= @myName %>
<%= @myScore %>
<table>
  <% @users.each do |user| %>
    <tr>
    <td><img width="100" src="<%= user[:icon] %>"></td>

    <td><%= user[:id] %></td>
    <td><%= user[:name] %></td>
    <td><%= user[:score] %></td>
    </tr>
  <% end %>
</table>